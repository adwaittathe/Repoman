<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="assets/css/index.css" />
    <link rel="stylesheet" href="popupS.css" />
    <title>RepoMan</title>
  </head>

  <body>
    <!-- NavBar -->
    <div>
      <%- include ("navbar") -%>
    </div>
    <!-- Main Content -->
    <div class="container mt-2">
      <div class="row">
        <!-- Content  -->
        <div class="col">
          <% for (var i=0; i< zipData.length; i++) { %>

          <div class="card">
            <a
              href="/company/<%=zipData[i]['id'] %>"
              id="company"
              class="card-header"
            >
              <b> <%= zipData[i]['Company Name'] %> </b>
            </a>

            <div class="card-body">
              <div class="row">
                <div class="col-sm-8">
                  <p class="card-text">
                    <b>Address </b> : <%= zipData[i]['Address1'] %> <%=
                    zipData[i]['Address2'] %>
                  </p>
                  <p class="card-text">
                    <b>Zipcode</b> : <%= zipData[i]['Zip'] %>
                  </p>
                  <p class="card-text">
                    <b>State</b> : <%= zipData[i]['State'] %>
                  </p>
                  <p class="card-text">
                    <b>Phone No</b> : <%= zipData[i]['Phone Number'] %>
                  </p>
                </div>
                <div class="col-sm-4">
                  <% if(zipData[i].companyLogoUrl){ %>
                  <img
                    src="<%= zipData[i].companyLogoUrl %>"
                    style="height: 150px; width: 150px;"
                  />
                  <% } %>
                </div>
              </div>
              <br />
              <a href="<%= zipData[i].companyMapUrl %>">Map packet</a>
              <br />
              <br />
              <% if(zipData[i].isInsured == 1){ %>
              <div class="row">
                <img
                  src="/assets/images/isInsured.png"
                  style="height: 60px; width: 230px;"
                />
              </div>
              <br />

              <% } %>

              <div class="row">
                <% for(var k=0; k< zipData[i]['URLList'].length; k++) { %>

                <img
                  src="<%= zipData[i]['URLList'][k] %>"
                  style="height: 50px; width: 110px;"
                />
                &nbsp;&nbsp; <% } %>
              </div>
            </div>
          </div>

          <br />

          <% } %>
        </div>
        <!-- Ad Space -->
        <div class="co-sm-3">
          <div id="vertical-ad-space">
            <% for(var i=0; i< SideBarImagesList.length; i++) { %>
            <img class="vertical-ad-img" src="<%= SideBarImagesList[i] %>" />
            <% } %>
          </div>
        </div>
      </div>
    </div>
    <!-- Footer -->
    <div id="footer">
      <div class="container">
        <div class="row">
          <div class="col-sm-2">
            <div class="footer-div">
              <ul class="footer-list" style="list-style-type: none;">
                <a href="/sitePolicy">
                  <li class="footer-text">Privacy Policy</li>
                </a>

                <a href="/customer">
                  <li class="footer-text">Your Account</li>
                </a>
                <a href="/aboutUSAWeb">
                  <li class="footer-text">About USA Web</li>
                </a>
              </ul>
            </div>
          </div>
          <div class="col-sm-2">
            <div class="footer-div">
              <ul class="footer-list" style="list-style-type: none;">
                <a href="/customer">
                  <li class="footer-text">Payment</li>
                </a>
                <a href="/contactUs">
                  <li class="footer-text">Contact Us</li>
                </a>
                <a href="/refund">
                  <li class="footer-text">Refund Policy</li>
                </a>
              </ul>
            </div>
          </div>
          <div class="col-sm-3"></div>
          <div class="col-sm-4">
            <img
              id="footer-img"
              src="assets/images/large-rm-logo.png"
              alt="RepoMan.com"
            />
          </div>
        </div>
        <hr />

        <div class="row">
          <div class="col-sm-8"></div>
          <div class="col-sm-4">
            <p class="footer-text">Copyright Â© USAWeb 1995-2019</p>
          </div>
        </div>
      </div>
    </div>

    <script
      src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
      integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
      integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
      integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
